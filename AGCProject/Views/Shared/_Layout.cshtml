<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Amarillo Gear Company</title>
    <link rel="stylesheet" type="text/css" href="~/lib/bootstrap/dist/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/css/navbar.css">
    <link rel="stylesheet" type="text/css" href="~/css/site.css">
</head>
<body>
    <nav class="navbar navbar-default">
            <div class="navbar-header">
                <a class="navbar-brand" href="/Home/Index">
                    <img alt="Brand" src="~/images/agclogo2.jpg">
                </a>
            </div>
            <div class="navbar-btn">
                <a class="navbar-link" href="/Home/Index">
                    Home
                </a> | <a class="navbar-link" href="/Home/Finish">
                    List of Spacer Cards
                </a>
            </div>
    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2017 - Jodie Rohrer</p>
        </footer>
    </div>
    @RenderSection("Scripts", required: false)
</body>
</html>
